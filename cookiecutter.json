{
    "project_name": "ideally a single word, or use underscores",
    "description": "",
    "project_name_dashes": "{{ cookiecutter.project_name.replace('_', '-') }}",
    "project_name_title": "{{ cookiecutter.project_name.replace('_', ' ').title() }}",
    "project_name_class": "{{ cookiecutter.project_name_title.replace(' ', '') }}",
    "author_email": "ewjoachim@gmail.com",
    "author_name": "Joachim Jablon",
    "cli_name": "{{ cookiecutter.project_name_dashes }}",
    "codeowners": "@ewjoachim",
    "documentation_codeowners": "",
    "copyright": "Joachim Jablon",
    "copyright_year": "{% now 'utc', '%Y' %}",
    "documentation_name": "{{ cookiecutter.project_name_title }}",
    "first_coc_responder_contact_information": "@ewjoachim - joachim at jablon.fr",
    "first_coc_responder_name": "Joachim Jablon",
    "second_coc_responder_contact_information": "",
    "second_coc_responder_name": "TODO",
    "github_owner": "ewjoachim",
    "github_repo": "{{ cookiecutter.project_name_dashes }}",
    "github_full_name": "{{ cookiecutter.github_owner }}/{{ cookiecutter.github_repo }}",
    "github_url": "https://github.com/{{ cookiecutter.github_full_name }}",
    "main_branch_name": "main",
    "package_keywords": "",
    "package_name": "{{ cookiecutter.project_name }}",
    "project_slug": "{{ cookiecutter.project_name }}",
    "pypi_name": "{{ cookiecutter.project_name_dashes }}",
    "python_version_main": "3.8",
    "python_versions": "3.6, 3.7, 3.8",
    "readthedocs_name": "{{ cookiecutter.project_name_dashes }}",
    "readthedocs_url": "https://{{ cookiecutter.project_name_dashes }}.readthedocs.io",
    "readthedocs_stable_url": "{{ cookiecutter.readthedocs_url }}/en/stable",
    "readthedocs_changelog_url": "{{ cookiecutter.readthedocs_stable_url }}/changelog.html",
    "top_level_exception_class": "{{ cookiecutter.project_name_class }}Exception",
    "_extensions": [
        "jinja2_time.TimeExtension"
    ]
}
